/* Name of package */
#define PACKAGE_NAME "limba"

/* Define to the address where bug reports for this package should be sent. */
#define PACKAGE_BUGREPORT "?"

/* Define to the home page for this package. */
#define PACKAGE_URL "http://limba.tenstral.net"

/* Define to the version of this package. */
#define PACKAGE_VERSION "@VERSION@"

/* Version number of package */
#define VERSION "@VERSION@"

#define LI_SW_ROOT_PREFIX "@SOFTWARE_ROOT_PREFIX@"

#define LI_SOFTWARE_ROOT "@SOFTWARE_INSTALL_ROOT@"

#define LI_KEYRING_ROOT "@CMAKE_INSTALL_FULL_LOCALSTATEDIR@/lib/limba/keyrings"

#endif /*CONFIG_H*/
